<Deducao>
	<TipoDeducao>{@TipoDeducao}</TipoDeducao>
	<DescricaoDeducao>{@DescricaoDeducao}</DescricaoDeducao>
	<IdentificacaoDocumentoDeducao>
		{@ifIdentificacaoNfse}
		<IdentificacaoNfse>
			<CodigoMunicipioGerador>{@CodigoMunicipioGerador}</CodigoMunicipioGerador>
			<NumeroNfse>{@NumeroNfse}</NumeroNfse>
			<CodigoVerificacao>{@CodigoVerificacao}</CodigoVerificacao>
		</IdentificacaoNfse>
		{@endifIdentificacaoNfse}
		{@ifIdentificacaoNfe}
		<IdentificacaoNfe>
			<NumeroNfe>{@NumeroNfe}</NumeroNfe>
			<UfNfe>{@UfNfe}</UfNfe>
			<ChaveAcessoNfe>{@ChaveAcessoNfe}</ChaveAcessoNfe>
		</IdentificacaoNfe>
		{@endifIdentificacaoNfe}
		{@ifOutroDocumento}
		<OutroDocumento>
			<IdentificacaoDocumento>{@IdentificacaoDocumento}</IdentificacaoDocumento>
		</OutroDocumento>
		{@endifOutroDocumento}
	</IdentificacaoDocumentoDeducao>
	<DadosFornecedor>
		{@ifIdentificacaoFornecedor}
		<IdentificacaoFornecedor>
			<CpfCnpj>
				{@ifCpf}
				<Cpf>{@Cpf}</Cpf>
				{@endifCpf}
				{@ifCnpj}
				<Cnpj>{@Cnpj}</Cnpj>
				{@endifCnpj}
			</CpfCnpj>
		</IdentificacaoFornecedor>
		{@endifIdentificacaoFornecedor}
		{@ifFornecedorExterior}
		<FornecedorExterior>
			{@ifNifFornecedor}
			<NifFornecedor>{@NifFornecedor}</NifFornecedor>
			{@endifNifFornecedor}
			<CodigoPais>{@CodigoPais}</CodigoPais>
		</FornecedorExterior>
		{@endifFornecedorExterior}
	</DadosFornecedor>
	<DataEmissao>{@DataEmissao}</DataEmissao>
	<ValorDedutivel>{@ValorDedutivel}</ValorDedutivel>
	<ValorUtilizadoDeducao>{@ValorUtilizadoDeducao}</ValorUtilizadoDeducao>
</Deducao>